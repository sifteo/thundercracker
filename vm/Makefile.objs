	src/Target/ARMGenRegisterInfo.inc \
	src/Target/ARMGenInstrInfo.inc \
	src/Target/ARMGenAsmWriter.inc \
	src/Target/ARMGenAsmMatcher.inc \
	src/Target/ARMGenDAGISel.inc \
