TABLEDEPS := src/Target/*.td
TABLEGEN = $(LLVM_BIN)/llvm-tblgen $(INCLUDES) -o $@ $<

src/Target/ARMGenRegisterInfo.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-register-info

src/Target/ARMGenInstrInfo.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-instr-info

src/Target/ARMGenAsmWriter.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-asm-writer

src/Target/ARMGenAsmMatcher.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-asm-matcher

src/Target/ARMGenDAGISel.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-dag-isel

src/Target/ARMGenSubtargetInfo.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-subtarget

src/Target/ARMGenCodeEmitter.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-emitter

src/Target/ARMGenCallingConv.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-callingconv

src/Target/ARMGenEDInfo.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-enhanced-disassembly-info

src/Target/ARMGenFastISel.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-fast-isel

src/Target/ARMGenMCCodeEmitter.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-emitter -mc-emitter
	
src/Target/ARMGenMCPseudoLowering.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-pseudo-lowering

src/Target/ARMGenDisassemblerTables.inc: src/Target/ARM.td $(TABLEDEPS)
	$(TABLEGEN) -gen-disassembler

